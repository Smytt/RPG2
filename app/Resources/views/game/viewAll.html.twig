{% extends "::base.html.twig" %}

{% block title %}All users{% endblock %}

{% block body %}
    <h1>All users:</h1>

    {% if users|length > 0 %}
        <ul>
            {% for user in users %}
                <li><a href="{{ path('attack_user', {'username': user.username}) }}">[ATTACK]</a> {{ user.username }}</li>
            {% endfor %}
        </ul>
    {% endif %}
{% endblock %}


